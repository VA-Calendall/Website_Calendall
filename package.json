    "vite-plugin-image-optimizer": "^1.1.7",
    "vite-plugin-sitemap": "^0.4.2"
  },
  "dependencies": {
    "@justinribeiro/lite-youtube": "^1.5.0",
    "aos": "^2.3.4",
    "bootstrap": "^5.2.3",
    "cmel_cookieconsent": "^0.0.7",
    "font-awesome": "^4.7.0",
    "sharp": "^0.33.0",
    "swiper": "^8.4.5",
    "vite-plugin-html-inject": "^1.0.1"
  }
